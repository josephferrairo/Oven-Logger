<br />
<div class="booyah-box">
  <%# %>
  <%= grid(@oven_logs_grid, upper_pagination_panel: true, ) do |g|

    g.column name: 'Customer', attribute: 'customer' do |oven_log|
      oven_log.customer
    end

    g.column name: 'Work Order', attribute: 'work_order' do |oven_log|
      oven_log.work_order
    end

    g.column name: 'Purchase Order', attribute: 'purchase_order' do |oven_log|
      oven_log.purchase_order
    end

    g.column name: 'Stress in(Date)', attribute: 'stress_in_date', filter_type: :bootstrap_datepicker do |oven_log|
      oven_log.stress_in_date
    end

    g.column name: 'Stress in', attribute: 'stress_in_time', filter_type: :false do |oven_log|
      oven_log.stress_in_time
    end

    g.column name: 'Stress out(Date)', attribute: 'stress_out_date', filter_type: :bootstrap_datepicker do |oven_log|
      oven_log.stress_out_date
    end

    g.column name: 'Stress out(Time)', attribute: 'stress_out_time', filter_type: :false do |oven_log|
      oven_log.stress_out_time
    end

    g.column name: 'Asset Number', attribute: 'stress_asset', filter_type: :false do |oven_log|
      oven_log.stress_asset
    end

    g.column name: 'HER in(Date)', attribute: 'her_in_date', filter_type: :bootstrap_datepicker do |oven_log|
      oven_log.her_in_date
    end

    g.column name: 'HER in(Time)', attribute: 'her_in_time', filter_type: :false do |oven_log|
      oven_log.her_in_time
    end

    g.column name: 'HER out(Date)', attribute: 'her_out_date', filter_type: :bootstrap_datepicker do |oven_log|
      oven_log.her_out_date
    end

    g.column name: 'HER out(Time)', attribute: 'her_out_time', filter_type: :false do |oven_log|
      oven_log.her_out_time
    end

    g.column name: 'Asset Number', attribute: 'her_asset', filter_type: :false do |oven_log|
      oven_log.her_asset
    end

    g.column name: 'Operator' do |oven_log|
      oven_log.operator
    end
  end %>
</div>
