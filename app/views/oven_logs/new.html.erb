<br  />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Add a New Customer</h1>
  <br  />
  <%= simple_form_for @oven_log, url: oven_logs_path do |f| %>
  <%= f.input :customer%>
  <%= f.input :work_order, label: 'Work Order'             %>
  <%= f.input :purchase_order, label: 'Purchase Order'     %>
  <%= f.input :stress_in_date, label: 'Stress in Date'     %>
  <%= f.input :stress_in_time, label: 'Stress in '         %>
  <%= f.input :stress_out_date, label: 'Stress out Date'   %>
  <%= f.input :stress_out_time, label: 'Stress out'        %>
  <%= f.input :stress_asset, label: 'Asset Number'         %>

  <%= f.input :her_in_date, label: 'HER in Date'           %>
  <%= f.input :her_in_time, label: 'HER in'                %>
  <%= f.input :her_out_date, label: 'HER out Date'         %>
  <%= f.input :her_out_time, label: 'HER out '             %>
  <%= f.input :her_asset, label: 'Asset Number'            %>
  <%= f.input :operator, label: 'Operator'                 %>

  <%= f.submit "Submit", class: 'btn btn-primary' %>
  <% end %>
</div>
